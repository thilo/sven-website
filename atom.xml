---
layout: nil
permalink: /feed/atom/
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>upstream agile - software</title>
  <link href="http://upstre.am/blog/"/>
  <link type="application/atom+xml" rel="self" href="http://upstre.am/feed/atom/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://upstre.am/</id>

  {% for post in site.posts %}
  <entry>
    <id>http://upstre.am{{ post.url }}</id>
    <link type="text/html" rel="alternate" href="http://upstre.am{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{{ post.author }}</name>
      <uri>http://upstre.am</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
